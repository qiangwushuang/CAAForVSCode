{
	"name": "caaextension",
	"displayName": "CAAExtension",
	"description": "CAA开发插件",
	"version": "0.0.1",
	"keywords": ["vscode","caa","catia"],
	"publisher": "qiangwushuang",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.buildModule",
				"title": "buildModule"
			},
			{
				"command":"extension.buildFramework",
				"title": "buildFramework"
		}],
		"keybindings":[{
			"command":"extension.buildModule",
			"key":"alt+2",
			"when":"editorTextFocus"	
		},
		{
			"command":"extension.buildFramework",
			"key":"alt+f2",
			"when":"editorTextFocus"	
		}],
		"menus":{
			"editor/context":[
				{
					"when":"editorFocus",
					"command":"extension.buildModule",
					"group":"navigation"
				},
				{
					"when":"editorFocus",
					"command":"extension.buildFramework",
					"group":"navigation"
				}]
		},
		"snippets": [
			{
				"language": "cpp",
				"path": "./snippets/caa.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
	

}
